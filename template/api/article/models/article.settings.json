{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "rawBody": {
      "type": "richtext",
      "required": true,
      "label": "Body"
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "category": {
      "model": "category"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "featured": {
      "type": "boolean"
    },
    "private": {
      "type": "boolean"
    },
    "tags": {
      "type": "component",
      "repeatable": true,
      "component": "shared.tags"
    },
    "keywords": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "thumbnailText": {
      "type": "string"
    },
    "author": {
      "model": "author"
    }
  }
}